<%= form_for @bookmark do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %>

  <%= f.label :url %>:
  <%= f.text_field :url %>

  <%= f.label :description %>:
  <%= f.text_field :description %>

  <%= f.label :category_id %>
  <%= f.select :category_id, Category.pluck(:name, :id) %>


  <%= f.submit %>
  <% end %>
